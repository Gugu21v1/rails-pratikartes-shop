<section
  class="coach-section rel z-1 pt-120 rpt-90 pb-100 rpb-70 bg-lighter"
  >
  <div class="container">
    <div class="row justify-content-center">
      <div class="col-xl-6 col-lg-7 col-md-8">
        <div class="section-title text-center mb-40">
          <h2>Alguns dos Nossos Cursos</h2>
        </div>
      </div>
    </div>
    <ul class="coach-filter mb-35">
      <li data-filter="*" class="current">Mostrar Todos</li>
      <li data-filter=".combate-contra-as-drogas">Combate às Drogas</li>
      <li data-filter=".concentracao">Concentração</li>
      <li data-filter=".memorizacao">Táticas de Memorização</li>
      <li data-filter=".super-leitura">Super Leitura</li>
      <li data-filter=".outro">Outros</li>
    </ul>
    <div class="row coach-active justify-content-center">
      <% if @produtos %>
        <% @produtos.each do |produto| %>
          <div class="col-lg-4 col-md-6 item <%= produto.tipo_curso.gsub(" ", "-").downcase.parameterize %>">
            <div class="coach-item wow fadeInUp delay-0-2s">
              <div class="coach-image">
                <a href="#" class="category"
                  ><%= produto.tipo_curso %></a
                  >
                <%= cl_image_tag produto.foto.key, height: 250, width: 370, crop: :fill, alt: "imagem produto" %>
              </div>
              <div class="coach-content">
                <h4><a href="#"><%= produto.nome %></a></h4>
                <div class="ratting-price">
                  <div class="ratting">
                    <i class="fas fa-star"></i>
                    <i class="fas fa-star"></i>
                    <i class="fas fa-star"></i>
                    <i class="fas fa-star"></i>
                    <i class="fas fa-star"></i>
                  </div>
                  <span class="price"><%= produto.valor %></span>
                </div>
                <ul class="coach-footer">
                  <li>
                    <i class="far fa-file-alt"></i><span><%= produto.modulos %></span>
                  </li>
                  <li><i class="far fa-user"></i><span><%= produto.autor %></span></li>
                </ul>
              </div>
            </div>
          </div>
        <% end %>
      <% else %>
        <h4 class="text-center">Sem Produtos</h4>
      <% end %>
    </div>
  </div>
</section>
<!-- Coach Section End -->
