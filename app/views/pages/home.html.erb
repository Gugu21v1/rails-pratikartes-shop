<section class="events-section rel z-1 py-130 rpy-100 bg-blue text-white">
  <div class="container">
    <div class="row justify-content-center">
      <div class="col-xl-7 col-lg-8 col-md-9">
        <div class="section-title text-center mb-55">
          <span class="sub-title mb-25">Nosso Blog!</span>
          <h2>
            Fique por Dentro das Últimas Notícias da Empresa e Eventos que
            Iremos Comparecer!
          </h2>
        </div>
      </div>
    </div>
    <div class="event-wrap">
      <% if @posts %>
        <% @posts.each do |post| %>
          <%= link_to post_path(post) do %>
            <div class="event-item wow fadeInUp delay-0-2s ajeito-post">
              <div class="image">
                <%= cl_image_tag post.img_url.key, height: 220, width: 310, crop: :fill %>
                <span class="date"><%= post.data %></span>
              </div>
              <div class="content">
                <h4><%= post.title %></h4>
                <span class="location"><%= post.body.split(/\\n/).join("<br />").html_safe %></span>
              </div>
            </div>
          <% end %>
        <% end %>
      <% else %>
        <h4 class="text-center">Nenhum Post</h4>
      <% end %>
    </div>
    <div class="blog-more-btn pt-30 text-center">
      <%= link_to "Ver Mais Notícias <i class='fas fa-arrow-right'></i>".html_safe, posts_path, class: "theme-btn style-three mais-noticias" %>
    </div>
  </div>
  <span class="bg-text">blog</span>
  <%= image_tag "shapes/rectangle-dots.png", alt:"shape", class: "rectangle-dots" %>
