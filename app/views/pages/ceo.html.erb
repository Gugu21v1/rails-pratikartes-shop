<section class="blog-section rpt-90 pb-100 rpb-70">
  <div class="container">
    <div class="section-title text-center mb-70">
      <span class="sub-title-two">Nosso Blog!</span>
      <h2>Últimas Notícias & <span>Eventos</span> Confirmados!</h2>
    </div>
    <div class="row justify-content-center">
      <% @posts.each do |post| %>
        <div class="col-lg-4 col-md-6">
          <div class="blog-item style-two middle-image wow fadeInUp delay-0-2s">
            <h4>
              <a href="blog-details.html"><%= post.title %></a>
            </h4>
            <ul class="blog-meta"></ul>
            <div class="blog-image">
              <%= cl_image_tag post.img_url.key, height: 200, width: 280, style: "object-fit: cover;" %>
              <span class="date"><%= post.data %></span>
            </div>
            <%= link_to "Ler Mais <i class='fas fa-arrow-right'></i>".html_safe, post_path(post), class: "read-more" %>
          </div>
        </div>
      <% end %>
    </div>
  </div>
</section>
<!-- Blog Section End -->
<script>
  document.querySelectorAll(".navbar-collapse.collapse.clearfix .navigation.clearfix>li").forEach((element) => {
    if(element.innerText.toUpperCase() === "CEO") {
      element.classList.add("current")
    } else {
      element.classList.remove("current")
    }
  });
</script>
