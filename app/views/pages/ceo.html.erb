              </li>
            </ul>
            <h4><a href="course-details.html"><%= produto.nome %></a></h4>
          </div>
        </div>
        <% if animacao == 6 %>
          <% animacao = 2 %>
        <% else %>
          <% animacao +=2 %>
        <% end %>
      <% end %>
    </div>
  </div>
